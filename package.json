{
  "name": "cv",
  "description": "Online static CV using React with GatsbyJS",
  "version": "0.3.0",
  "homepage": "https://raulfdmcv.surge.sh",
  "author": "Raul Melo <melo.raulf@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf public",
    "build": "gatsby build",
    "predeploy": "npm run build",
    "deploy": "surge -p public/ -d https://raulfdmcv.surge.sh",
    "start": "gatsby develop",
    "format": "prettier --write 'src/**/*.js'"
  },
  "dependencies": {
    "gatsby": "^1.9.277",
    "gatsby-link": "^1.6.46",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-google-fonts": "^0.0.4",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "linkifyjs": "^2.1.7",
    "modern-normalize": "^0.5.0",
    "moment": "^2.22.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "prettier": "^1.13.7",
    "surge": "^0.20.1",
    "rimraf": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  }
}
